      "multiple": false
    },
    "endpoint": {
      "type": "string",
      "multiple": false
    },
    "secretId": {
      "type": "string",
      "multiple": false
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    }
  }
}
